class Solution {
public:
    double myPow(double x, int n) {
        double result = 1.0;
        long long N = n; 
        if (N < 0) {
            x = 1 / x;
            N = -N;
        }
        for (long long i = 0; i < N; i++) {
            result *= x;
        }
        return result;
    }
};
